			<column name="shopfinder_id"/>
		</constraint>
		<column name="shop_name" nullable="true" xsi:type="varchar" comment="shop_name" length="255"/>
		<column name="identifier" nullable="true" xsi:type="varchar" comment="identifier" length="255"/>
		<column name="country" nullable="true" xsi:type="varchar" comment="country" length="255"/>
		<column name="image" nullable="true" xsi:type="text" comment="image"/>
		<column name="longitude_latitude" nullable="true" xsi:type="varchar" comment="longitude_latitude" length="255"/>
	</table>
</schema>
